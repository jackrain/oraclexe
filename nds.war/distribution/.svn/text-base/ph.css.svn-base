#menu {
		  font-family:Arial;
		  font-size:12px;
		  }
		  
	    #menu a, #menu a:visited {
		  display:block; 
		  float:left;
		  position:relative;
		  background-color:#cfe1ef;
		  color:#333; 
		  text-decoration:none;
		  padding:2px 10px;
		  margin:1px 0 0 5px; 
		  }
		 
		 #menu a span{
		 height:0;
		 width:0;
		 border-bottom:solid 6px #cfe1ef;
		 border-left:solid 6px #ccc;
		 position:absolute;
		 top:0;
		 left:0;
		 overflow:hidden;
		 }
		  
		#menu a:hover{
		background-color: #024770;
		color:#fff;
		}
		
		#menu a:hover span{
		border-bottom:solid 6px #024770;
		}

