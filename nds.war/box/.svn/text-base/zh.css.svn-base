body{
 margin:0px auto;
 padding:0px;
 height:100%;
 text-align:left;
 border:0;
 font-family:Arial, Helvetica, sans-serif;
 font-size:12px;
 color:#000;
}
.body-bg{background:url(images/bg.gif) repeat-x scroll;}

a{
	TEXT-DECORATION: underline;
	font-size: 12px;
	color: #000;
}

a:hover{
    font-size: 12px;
    color:#185589;
    TEXT-DECORATION: underline;
	}

