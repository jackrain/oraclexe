<div id="layout-content-outer-decoration">
	<div id="layout-content-inner-decoration">
		<div id="layout-content-container">
		<table cellpadding="0" cellspacing="0" width="100%">
			<tr valign="top">
				<td valign="top" colspan="5"><!--column-6 not 5 to escape margin definition-->
				$processor.processColumn("column-6")
				</td>
			</tr>
			<tr>
				<td valign="top" width="20%" rowspan="2">
				$processor.processColumn("column-1")
				</td>
				<td valign="top" rowspan="2" class="layout-column-spacer" width="1%">
					<div>&nbsp;</div>
				</td>
				<td valign="top" width="58%">
				$processor.processColumn("column-2")
				</td>
				<td valign="top" class="layout-column-spacer" width="1%"><div>&nbsp;</div>
				</td>
				<td valign="top" width="20%">
				$processor.processColumn("column-3")
				</td>
			</tr>
			<tr>
				<td valign="top" colspan="3">
				$processor.processColumn("column-4")
				</td>
			</tr>
		</table>
		</div>
	</div>
</div>
