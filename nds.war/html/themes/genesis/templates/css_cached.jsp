	margin: 0 auto 0 auto;
}

#layout-top-banner {
	height: 75px;
	position: relative;
	text-align: left;
	z-index: 3;
}

#layout-user-menu {
	position: absolute;
	text-align: right;
	top: 0px;
	right: 0px;
}

#layout-my-places {
	position: relative;
	right: 0;
	width: 200px;
	padding: 0;
}

#layout-my-places form { text-align: right; }

#layout-my-places ul {
	background-color: <%= colorScheme.getLayoutBg() %>;
	border: 1px solid <%= colorScheme.getPortletTitleBg() %>;
	position: absolute;
	text-align: left;
	right: 0;
	top: 0;
	margin: 0;
}

#layout-my-places ul li {
	padding: 2px;
}

#layout-global-search {
	position: absolute;
	top: 5px;
	right: 5px;
}

#layout-corner-ul { }
#layout-corner-ur { }
#layout-corner-bl { }
#layout-corner-br { }
#layout-corner-2-bl { }
#layout-corner-2-br { }
#layout-top-decoration { }
#layout-bottom-decoration-2 { }

