	top: 0px;
	width: <%= TAB_IMAGE_SIDE_WIDTH %>;
}

.layout-nav-tab-left-end {
	background: url(<%= themeDisplay.getPathThemeImage() %>/custom/tabs/tab-medium-left-end.gif) no-repeat;
