*{margin:0; padding:0;}
html{ font-size:12px; font-family:Arial, Helvetica, sans-serif;}

body{ margin:0; padding:0; background:#fff;}
@-moz-document url-prefix() {body {  margin:0; padding:0; background:#fff;overflow-x:auto;overflow-y:auto; }}

body ul{ margin:0; padding:0;}
body ul li{ list-style:none;}
form,p,h1,h2,h3,h4,h5,h6,ul,li,a,input,map{margin:0; padding:0; list-style:none; outline:none;}
img{border:none; margin:0; padding:0;}

